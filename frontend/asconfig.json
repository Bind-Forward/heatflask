{
  "targets": {
    "development": {
      "binaryFile": "dist/dev.wasm",
      "textFile": "dist/dev.wat",
      "sourceMap": true,
      "debug": true,
      "enable": ["bulk-memory", "simd"]
    },
    "production": {
      "binaryFile": "dist/prod.wasm",
      "textFile": "dist/prod.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 1,
      "converge": false,
      "noAssert": false,
      "enable": ["bulk-memory", "simd"]
    }
  },
  "options": {}
}
