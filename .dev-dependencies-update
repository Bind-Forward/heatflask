#!/bin/env sh

## This script installs/updates all frontend and backend dependecies
##  to the the versions specified in
##   frontend/package.json and backend/requirements.txt

# Install frontend dependencies
echo "Installing Frontend dependencies..."
cd frontend
npm install &

echo "Installing backend dependencies..."
cd ../backend
./.dev-install-backend

echo "You will still need to install data-stores redis, postgresql, and MongoDB"

